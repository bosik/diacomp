            <solid android:color="@color/diary_blood_postprand_sel" />
            <stroke android:width="1dp" android:color="@color/diary_border_sel" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/diary_blood_postprand_sel" />
            <stroke android:width="1dp" android:color="@color/diary_border" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/diary_blood_postprand" />
            <stroke android:width="1dp" android:color="@color/diary_border" />
        </shape>
    </item>
</selector>